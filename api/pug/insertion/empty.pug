      code.section__.
        &lt;ul id="list" class="list"&gt;
            &lt;li class="item about"&gt;About&lt;/li&gt;
        &lt;/ul&gt;

        // 引入 DOM 模块（中的所有工具方法）
        import DOM from '@yaohaixiao/dom.js/dom
        // 引用 detach 单个方法
        import empty from '@yaohaixiao/dom.js/empty'

        const $list = DOM.byId('#list')

        // 清空 $list 节点中的所有子节点
        DOM.empty($list)
        empty($list)
