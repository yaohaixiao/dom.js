    pre.section__pre
      code.section__.
        &lt;ul id="list" class="list"&gt;
            &lt;li id="home" class="item active"&gt;Home&lt;/li&gt;
            &lt;li class="item blog"&gt;Blog&lt;/li&gt;
            &lt;li class="item"&gt;Projects&lt;/li&gt;
            &lt;li class="item"&gt;About&lt;/li&gt;
        &lt;/ul&gt;

        // 引入 DOM 模块（中的所有工具方法）
        import DOM from '@yaohaixiao/dom.js/dom
        // 引用 filter 单个方法
        import filter from '@yaohaixiao/dom.js/filter'

        const $items = DOM.byClass('.item')

        // 通过选择器获取子元素
        const $active = DOM.filter($items, '.active')

        // 通过回调函数获取子元素
        const $blog = filter($items, (item) => {
            return item.classList.contains('blog')
        })
